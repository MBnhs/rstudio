1693370825589:calcula_coeficiente_sus <- function(linha) {
1693370825598:soma_dos_items <- 0
1693370825599:for (item in linha) {
1693370825600:numeric_item <- as.numeric(item)  # Convertendo para número
1693370825600:if (numeric_item > 0) {
1693370825600:cat("Item:", numeric_item)
1693370825601:if (!is.na(numeric_item)) {  # Verifica se a conversão foi bem-sucedida
1693370825601:if (numeric_item %% 2 == 0) {
1693370825601:numeric_item = abs(numeric_item - 5)
1693370825602:} else {
1693370825602:numeric_item = numeric_item - 1
1693370825602:}
1693370825602:soma_dos_items <- soma_dos_items + numeric_item
1693370825603:#cat(" passa a ser ", numeric_item)
1693370825603:cat("\n")
1693370825603:} else {
1693370825603:cat(" (não é um número)\n")
1693370825604:}
1693370825604:}
1693370825604:}
1693370825604:#cat("--------\n")
1693370825605:#cat(soma_dos_items, "\n")
1693370825605:soma_dos_items <- soma_dos_items * 2.5
1693370825605:#cat("--------\n")
1693370825605:#cat("Coeficiente de usabilidade:", soma_dos_items ,"\n")
1693370825605:return(soma_dos_items)
1693370825606:}
1693370825606:resultado <- apply(dados_selecionados[1, ], 1, calcula_coeficiente_sus)
