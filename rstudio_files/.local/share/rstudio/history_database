1693493959495:calcula_coeficiente_sus <- function(linha) {
1693493959495:soma_dos_items <- 0
1693493959496:for (item in linha) {
1693493959496:numeric_item <- as.numeric(item)  # Convertendo para número
1693493959497:if (numeric_item > 0) {
1693493959497:if (!is.na(numeric_item)) {  # Verifica se a conversão foi bem-sucedida
1693493959497:soma_dos_items <- soma_dos_items + numeric_item
1693493959497:}
1693493959497:}
1693493959498:}
1693493959498:soma_dos_items <- soma_dos_items * 2.5
1693493959498:return(soma_dos_items)
1693493959498:}
1693493959498:resultado <- apply(dados_selecionados[1, ], 1, calcula_coeficiente_sus)
