1716890953553:participantes <- read.csv("/home/rstudio/participantes.csv")
1716890976581:participantes <- read.csv("/home/rstudio/participantes.csv")
1716890976582:colunas_selecionadas <- c("Q1",	"Q2",	"Q3",	"Q4",	"Q5",	"Q6",	"Q7",	"Q8",	"Q9",	"Q10")
1716890976582:dados_selecionados <- participantes[, colunas_selecionadas]
1716890976583:calcula_coeficiente_sus <- function(linha) {
1716890976583:soma_dos_items <- 0
1716890976583:questao = 0
1716890976583:for (item in linha) {
1716890976583:questao = questao + 1
1716890976583:numeric_item <- as.numeric(item)  # Convertendo para número
1716890976583:if (numeric_item > 0) {
1716890976583:if (!is.na(numeric_item)) {  # Verifica se a conversão foi bem-sucedida
1716890976583:if (questao %% 2 == 0) {
1716890976583:numeric_item = abs(numeric_item - 5)
1716890976584:} else {
1716890976584:numeric_item = numeric_item - 1
1716890976584:}
1716890976584:soma_dos_items <- soma_dos_items + numeric_item
1716890976584:}
1716890976584:}
1716890976584:}
1716890976584:soma_dos_items <- soma_dos_items * 2.5
1716890976584:return(soma_dos_items)
1716890976584:}
1716890976585:resultado <- apply(dados_selecionados[1, ], 1, calcula_coeficiente_sus)
1716890976592:num_linhas <- nrow(dados_selecionados)
1716890976593:resultados <- numeric(70)
1716890976593:for (i in 1:num_linhas) {
1716890976593:linha_atual <- dados_selecionados[i, ]
1716890976593:resultado <- calcula_coeficiente_sus(linha_atual)
1716890976593:print(resultado)
1716890976593:resultados[i] <- resultado
1716890976593:}
