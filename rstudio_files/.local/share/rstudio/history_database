1693026073613:options(repos = "http://cran-r.c3sl.ufpr.br/")
1693026073621:# sessionInfo()
1693026073621:# dados <- read.csv("dados.csv")
1693026073621:participantes <- read.csv("/home/rstudio/participantes.csv")
1693026073623:# head(participantes, 5)
1693026073623:# install.packages("dplyr")
1693026073623:# library(dplyr)
1693026073624:# c(arrange(unique(select(participantes,idade)), idade))
1693026073624:# chooseCRANmirror()
1693026073624:# install.packages("glue")
1693026073625:# library(glue)
1693026073625:print("Distribuição de frequências para variáveis qualitativas - Q1")
1693026073625:table(participantes$Q1)
1693026073628:print("Distribuição (percentual) de frequências para variáveis qualitativas - Q1")
1693026073628:# prop.table(table(participantes$Q1)) * 100
1693026073628:dist_freq_qualitativas <- cbind(freq = table(participantes$Q1), percent = prop.table(table(participantes$Q1)) * 100)
1693026073629:colnames(dist_freq_qualitativas) <- c("Frequencia", "Porcentagem (%)")
1693026073629:dist_freq_qualitativas
1693026073630:frequencia <- table(participantes$Q1, participantes$idade)
1693026073630:rownames(frequencia) <- c('Q10', 'Q11', 'Q12', 'Q13', 'Q14', 'Q15')
1693026073631:frequencia <- cbind(frequencia)
1693026073631:frequencia
1693026073632:# print("Media da idade dos participantes")
1693026073632:# mediaIdade <- tapply(participantes$idade, list(participantes$Q1, participantes$Q2, participantes$Q3), mean)
1693026073633:# mediaIdade
1693026073633:print ("Faixas etarias")
1693026073633:classes <- c(min(participantes$idade),30,40,50,60, max(participantes$idade))
1693026073633:labels <- c('Ate 30 anos', 'Entre 31 e 40 anos', 'Entre 41 e 50 anos', 'Entre 51 e 60 anos', 'Acima de 60 anos')
1693026073633:frequencia <- table (cut(
1693026073634:x = participantes$idade,
1693026073634:breaks = classes,
1693026073634:labels = labels,
1693026073634:include.lowest = TRUE
1693026073634:))
1693026073635:frequencia
1693026073636:n <- nrow(participantes)
1693026073636:k <- 1 + (10/3) * log10(n)
1693026073636:k <- round(k)
1693026073637:print("K redondo")
1693026073637:k
1693026073637:frequencia <- table (cut(
1693026073637:x = participantes$idade,
1693026073637:breaks = k,
1693026073638:include.lowest = TRUE
1693026073638:))
1693026073639:frequencia
1693026073639:percentual <- prop.table(frequencia) * 100
1693026073640:print ("Distribuicao de frequencia quantitativas amplitude fixa")
1693026073640:dist_freq_quantitativas_amplitude_fixa <- cbind('Frequencia' = frequencia, 'Porcentagem (%)' = percentual)
1693026073640:dist_freq_quantitativas_amplitude_fixa
1693026073641:hist(participantes$idade)
