1693500441339:if (numeric_item > 0) {
1693500441340:if (questao == 1 || questao == 3 || questao == 5 || questao == 7 || questao == 9) {
1693500441340:numeric_item = numeric_item - 1
1693500441340:} else if (questao == 2 || questao == 4 || questao == 6 || questao == 8 || questao == 10) {
1693500441341:numeric_item = abs(numeric_item - 5)
1693500441341:}
1693500441341:soma_dos_items <- soma_dos_items + numeric_item
1693500441342:}
1693500441342:}
1693500441342:soma_dos_items <- soma_dos_items * 2.5
1693500441343:return(soma_dos_items)
1693500441343:}
1693500441344:resultado <- apply(dados_selecionados[1, ], 1, calcula_coeficiente_sus)
