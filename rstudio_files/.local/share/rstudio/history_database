1693500730063:if (numeric_item > 0) {
1693500730063:if (!is.na(numeric_item)) {  # Verifica se a conversão foi bem-sucedida
1693500730064:if (numeric_item %% 2 == 0) {
1693500730064:numeric_item = abs(numeric_item - 5)
1693500730065:} else {
1693500730065:numeric_item = numeric_item - 1
1693500730065:}
1693500730065:soma_dos_items <- soma_dos_items + numeric_item
1693500730066:}
1693500730066:}
1693500730067:}
